<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>servicemethodexample</servlet-name>
        <servlet-class>com.telusko.AddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servicemethodexample</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>dogetdopostexample</servlet-name>
        <servlet-class>com.telusko.DoGetDoPost</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dogetdopostexample</servlet-name>
        <url-pattern>/addbypostreq</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>requestdispatcherexample</servlet-name>
        <servlet-class>com.telusko.callingaservlet.RequestDispatcherExample</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>requestdispatcherexample</servlet-name>
        <url-pattern>/requestdispatcher</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>squareservlet</servlet-name>
        <servlet-class>com.telusko.callingaservlet.SquareServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>squareservlet</servlet-name>
        <url-pattern>/square</url-pattern>
    </servlet-mapping>


</web-app>